<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gaoling.admin.system.dao.ResponseInfoDao">

	<!-- 查询响应内容 -->
	<select id="queryResponseTextCn" resultType="String">
		select text_cn from response_info where class_name=#{className} and `code`=#{code} and state=1
	</select>

</mapper>  
